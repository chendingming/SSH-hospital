<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ding.entity">
    <class name="com.ding.entity.Record" table="t_record">
        <id name="id" column="id">
            <generator class="native"></generator>
        </id>
        <property name="body" column="body"></property>
        <property name="medical" column="medical"></property>
        <property name="order_id" column="order_id"></property>
    </class>
</hibernate-mapping>